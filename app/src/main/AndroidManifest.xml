<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.sean.androidpos" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity">
        </activity>

        <activity android:name=".CheckoutActivity">
        </activity>

        <activity android:name=".StockCheckActivity">
        </activity>

        <activity android:name=".StockManagementActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".EmpManagementActivity"
            android:windowSoftInputMode="stateHidden">
        </activity>

        <activity android:name=".BarcodeGenActivity">
        </activity>

        <activity android:name=".SettingActivity">
        </activity>

    </application>

</manifest>
